function Сотрудник(Имя, Отдел, Телефон, Зарплата) {
    this.Имя = Имя;
    this.Отдел = Отдел;
    this.Телефон = Телефон;
    this.Зарплата = Зарплата;
}

var employee = new Сотрудник("Иванов Иван", "Отдел разработки", "123-456-789", 50000);
employee.адрес = "ул. Примерная, д. 123";

console.log("Имя: " + employee.Имя);
console.log("Отдел: " + employee.Отдел);
console.log("Телефон: " + employee.Телефон);
console.log("Зарплата: " + employee.Зарплата);
console.log("Адрес: " + employee.адрес);
