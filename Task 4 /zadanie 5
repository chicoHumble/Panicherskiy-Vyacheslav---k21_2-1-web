var Сотрудник = {
    Имя: "",
    Отдел: "",
    Телефон: "",
    Зарплата: 0,

    отобразитьДанные: function () {
        console.log("Имя: " + this.Имя);
        console.log("Отдел: " + this.Отдел);
        console.log("Телефон: " + this.Телефон);
        console.log("Зарплата: " + this.Зарплата);
    }
};

Сотрудник.Имя = "Иванов Иван";
Сотрудник.Отдел = "Отдел разработки";
Сотрудник.Телефон = "123-456-789";
Сотрудник.Зарплата = 50000;

Сотрудник.отобразитьДанные();
