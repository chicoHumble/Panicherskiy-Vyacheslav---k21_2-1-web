// Условие: Написать скрипт, который по заданным значениям чисел a и b находит a в степени b. 
// Допустимые значения переменных: a может быть любым числом, а b должен быть целым числом.

const a = 4; 
const b = 2; 

let result;

if (Number.isInteger(b)) {
    result = Math.pow(a, b);
} else {
    result = "Ошибка: b должно быть целым числом.";
}

console.log(result);
